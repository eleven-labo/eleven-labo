generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Formation {
  id         Int      @id @default(autoincrement())
  code       String
  name       String
  createdAt DateTime @default(now())
}

model Theme {
  id        Int      @id @default(autoincrement())
  title     String
  likeCount String
  createdAt DateTime @default(now())
}
