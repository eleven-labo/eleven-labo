generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Team {
  code              String
  name              String
  background_color  String
  border_color      String
  color             String
  text_shadow_color String
}

model Themes {
  title      String
  like_count Int
  post_count Int
  created_at DateTime @default(now())
}
